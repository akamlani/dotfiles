[user]
	name = Ari Kamlani
	email = <akamlani@gmail.com>
[color]
	ui = true
[branch]
	autosetuprebase = always
[push]
	default = tracking
[format]
    #pretty=format:%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset
[init]
	templatedir = ~/.git_template
[alias]
	ctags = !.git/hooks/ctags
  pu    = !"git fetch origin -v; git fetch upstream -v; git merge --ff upstream/master"
  welp  = reset --soft HEAD~1
  workprofile = config user.email \"akamlani@beyond.ai\"
[merge]
	tool = vimdiff
	conflictstyle = diff3
  renamelimit = 2424
[mergetool]
	keepBackup = false
[diff]
  tool = vimdiff
	conflictstyle = diff3
